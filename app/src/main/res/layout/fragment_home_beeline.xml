<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home_beeline">

    <ImageView
        android:id="@+id/beeline"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:src="@drawable/beeline"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5"
        tools:ignore="MissingConstraints" />

        <androidx.cardview.widget.CardView
            android:clickable="true"
            android:focusable="true"
            app:cardElevation="10dp"
            app:layout_constraintTop_toBottomOf="@+id/beeline"
            app:layout_constraintBottom_toTopOf="@id/card_internet"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/card_daqiqa"
            android:id="@+id/card_tarif"
            android:layout_width="150dp"
            android:foreground="?android:attr/selectableItemBackground"
            android:layout_height="150dp"
            tools:ignore="MissingConstraints">
            <ImageView
                android:layout_marginTop="10dp"
                android:layout_marginStart="25dp"
                android:src="@drawable/list_tarif"
                android:layout_width="100dp"
                android:layout_height="100dp"/>
            <TextView
                android:text="Tariflar"
                android:textSize="20sp"
                android:gravity="center"
                android:layout_marginTop="100dp"
                android:layout_width="match_parent"
                android:layout_height="30dp"/>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            app:cardElevation="10dp"
            android:clickable="true"
            android:focusable="true"
            android:foreground="?android:attr/selectableItemBackground"
            app:layout_constraintLeft_toRightOf="@+id/card_tarif"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/beeline"
            app:layout_constraintBottom_toTopOf="@+id/card_xizmat"
            android:id="@+id/card_daqiqa"
            android:layout_width="150dp"
            android:layout_height="150dp"
            tools:ignore="MissingConstraints">
            <ImageView
                android:layout_marginTop="10dp"
                android:layout_marginStart="25dp"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/daqiqa" />
            <TextView
                android:text="Daqiqalar"
                android:textSize="20sp"
                android:gravity="center"
                android:layout_marginTop="100dp"
                android:layout_width="match_parent"
                android:layout_height="30dp"/>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            app:cardElevation="10dp"
            android:clickable="true"
            android:focusable="true"
            android:foreground="?android:attr/selectableItemBackground"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/card_tarif"
            app:layout_constraintRight_toLeftOf="@+id/card_xizmat"
            app:layout_constraintBottom_toTopOf="@+id/card_sms"
            android:id="@+id/card_internet"
            android:layout_width="150dp"
            android:layout_height="150dp"
            tools:ignore="MissingConstraints">
            <ImageView
                android:layout_marginTop="10dp"
                android:layout_marginStart="25dp"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/internet" />
            <TextView
                android:text="Internet"
                android:textSize="20sp"
                android:gravity="center"
                android:layout_marginTop="100dp"
                android:layout_width="match_parent"
                android:layout_height="30dp"/>

        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:clickable="true"
            android:focusable="true"
            app:cardElevation="10dp"
            android:foreground="?android:attr/selectableItemBackground"
            app:layout_constraintTop_toBottomOf="@id/card_daqiqa"
            app:layout_constraintLeft_toRightOf="@+id/card_internet"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/card_yangilik"
            android:id="@+id/card_xizmat"
            android:layout_width="150dp"
            android:layout_height="150dp"
            tools:ignore="MissingConstraints">
            <ImageView
                android:layout_marginTop="10dp"
                android:layout_marginStart="25dp"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/control" />
            <TextView
                android:text="Xizmatlar"
                android:textSize="20sp"
                android:gravity="center"
                android:layout_marginTop="100dp"
                android:layout_width="match_parent"
                android:layout_height="30dp"/>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:clickable="true"
            android:focusable="true"
            android:foreground="?android:attr/selectableItemBackground"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@id/card_internet"
            app:layout_constraintRight_toLeftOf="@+id/card_yangilik"
            android:id="@+id/card_sms"
            android:layout_width="150dp"
            android:layout_height="150dp"
            app:cardElevation="10dp"
            tools:ignore="MissingConstraints">
            <ImageView
                android:layout_marginTop="10dp"
                android:layout_marginStart="25dp"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/sms" />
            <TextView
                android:text="Smslar"
                android:textSize="20sp"
                android:gravity="center"
                android:layout_marginTop="100dp"
                android:layout_width="match_parent"
                android:layout_height="30dp"/>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:clickable="true"
            android:focusable="true"
            android:foreground="?android:attr/selectableItemBackground"
            app:layout_constraintLeft_toRightOf="@+id/card_sms"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/card_xizmat"
            app:layout_constraintBottom_toBottomOf="parent"
            android:id="@+id/card_yangilik"
            app:cardElevation="10dp"
            android:layout_width="150dp"
            android:layout_height="150dp"
            tools:ignore="MissingConstraints">
            <ImageView
                android:layout_marginTop="10dp"
                android:layout_marginStart="25dp"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@drawable/news" />
            <TextView
                android:text="Yangiliklar"
                android:textSize="20sp"
                android:gravity="center"
                android:layout_marginTop="100dp"
                android:layout_width="match_parent"
                android:layout_height="30dp"/>
        </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>